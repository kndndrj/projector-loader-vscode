{
    "runtime.version": "LuaJIT",
    "diagnostics": {
        "enable": true,
        "globals": [
          "vim",
          "use",
        ]
    },
    "workspace": {
        "library": {
            "runtime/lua": true
        },
        "maxPreload": 2000,
        "preloadFileSize": 1000
    },
    "format": {
        "enable": true,
    "defaultConfig":{
        "indent_style":"space",
        "indent_size":"2"
    }
    },
    "telemetry.enable": false
}
